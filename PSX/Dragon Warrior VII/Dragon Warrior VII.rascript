function trigger_serial_killer()
{
    trigger = trigger_in_game()
    alt_clause = always_false()
    target_kills = 100
    
    // bit2-bit11 = number of kills (12-bit read >> 2)
    for i in range(1, 303)
    {
        ok = 1
        for j in not_in_book
        {
            if (i == j)
                ok = 0
        }
    
        have_heart = 0
        for j in have_a_heart
        {
            if (i == j)
                have_heart = 1
        }
        
        if (ok == 1)
        {
            if (have_heart == 0)
                alt_clause = alt_clause || (measured(monster_kills(i) >= 100) && prev(monster_kills(i)) < 100)
            else
                alt_clause = alt_clause || 
                     (byte(0x11386 + i * 2) >= 0x91 &&
                      low4(0x11386 + i * 2 + 1) == 1 &&
                      prev(byte(0x11386 + i * 2)) != byte(0x11386 + i * 2))
        }
    }
    
    return trigger && alt_clause
}

