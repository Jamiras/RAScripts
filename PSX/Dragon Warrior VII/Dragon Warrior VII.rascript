// ===========================

function trigger_in_game() => dword(0x0D57C4) != 0
function trigger_in_battle() => trigger_in_game() && byte(0x010069) == 0xC0
function playtime() => dword(0x1003C)
function current_music() => dword(0x010038)

rich_presence_conditional_display(!trigger_in_game(), "The journey has not begun")

rich_presence_display("A level {0} {1} is exploring {2} ({3})", 
    rich_presence_value("Level", byte(tbyte(0x0112F8) + 0x005C)),
    rich_presence_lookup("Class", byte(tbyte(0x0112F8) + 0x0079), classes),
    rich_presence_lookup("Location", current_location(), locations),
    rich_presence_value("Time", playtime(), format="SECS_AS_MINS")
)

function party_char_data_pointer(index) => word(0x112F8 + index * 4) + 0x010000
function char_data_pointer(char_id) => 0x10C38 + char_id * 0x120
