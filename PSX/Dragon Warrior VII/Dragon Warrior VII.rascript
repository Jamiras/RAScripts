function trigger_zoologist(target_kills)
{
    trigger = trigger_in_game()
    kill_count = 0
    for i in range(1, 303)
    {
        ok = 1
        for j in not_in_book
        {
            if (i == j)
                ok = 0
        }

        if (ok == 1)
            kill_count = kill_count + bit0(0x11386 + i * 2)
    }
    
    for i in bosses_in_book
        kill_count = kill_count + bit0(0x11386 + i * 2)

    return trigger && measured(kill_count >= target_kills)
}

