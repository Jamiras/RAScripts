function wrf_achievement(title, description, points, offset)
{
    winner_clause = always_false()
    for i in range(1, 6)
        winner_clause = winner_clause || (bit(offset + i, 0x1015E) == 1 && prev(bit(offset + i, 0x1015E)) == 0)

    achievement(title, description, points,
        trigger = trigger_in_game() && current_location() == location_litorud && 
                  winner_clause
    )
}

