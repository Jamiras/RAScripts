function achievement_park_monsters(title, description, count, points)
{
    bit_total = 0
    bit_change = always_false()
    for addr in range(0x11710, 0x11736, step=4)
    {
        for bit in range(0, 31)
            bit_total = bit_total + bit(bit, addr)
            
        bit_change = bit_change || dword(addr) > prev(dword(addr))
    }
        
    achievement(title, description, points,
        trigger = trigger_in_battle() && measured(bit_total >= count) && bit_change
    )
}

